pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                bat "mvn clean install -f cicdpipelineapp/pom.xml"
            }
        }
        stage('Test'){
        	steps {
        		bat "mvn test -f cicdpipelineapp/"
        	}
        }
        stage('Package'){
        	steps {
        		bat "mvn package -f cicdpipelineapp/"
        	}
        }
        stage('Deploy') {
            steps {
            	echo "Deploying........"
                bat 'copy "C:\\Program Files (x86)\\Jenkins\\workspace\\cicd-pipeline-stages\\cicdpipelineapp\\target\\"  "ec2-3-134-86-33.us-east-2.compute.amazonaws.com:/usr/java/apache-tomcat-8.5.61/webapps/"' 
            }
        }
        
    }
}
