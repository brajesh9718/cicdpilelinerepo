pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                bat "mvn clean install -f cicdpipelineapp/pom.xml"
            }
        }
        stage('Test'){
        	steps {
        		bat "mvn test -f cicdpipelineapp/"
        	}
        }
        stage('Package'){
        	steps {
        		bat "mvn package -f cicdpipelineapp/"
        	}
        }
        stage('Deploy') {
            steps {
            	echo "Deploying........"
                bat 'copy "C:\\Program Files (x86)\\Jenkins\\workspace\\cicd-pipeline-stages\\cicdpipelineapp\\target\\"  "http://18.223.121.184:8080/webapps"' 
            }
        }
        
    }
}
