pipeline {
    agent any
    stages {
        stage('clean & install') {
            steps {
                bat "mvn clean install -f cicdpipelineapp/pom.xml"
            }
        }
        stage('testing'){
        	steps {
        		bat "mvn test -f cicdpipelineapp/"
        	}
        }
        stage('packaging'){
        	steps {
        		bat "mvn package -f cicdpipelineapp/"
        	}
        }
        stage('deploying'){
        	steps {
        		echo "Deploy begins......."
        	}
        }
    }
}
